	<!-- Librerie -->
	<classpathentry exported="true" kind="lib" path="progetto_my_life/lib/jackson-annotations-2.17.1.jar">
		<attributes>
			<attribute name="module" value="true"/>
		</attributes>
	</classpathentry>
	<classpathentry exported="true" kind="lib" path="progetto_my_life/lib/jackson-core-2.17.1.jar">
		<attributes>
			<attribute name="module" value="true"/>
		</attributes>
	</classpathentry>
	<classpathentry exported="true" kind="lib" path="progetto_my_life/lib/jackson-databind-2.17.1.jar">
		<attributes>
			<attribute name="module" value="true"/>
		</attributes>
	</classpathentry>

	<classpathentry exported="true" kind="con" path="org.eclipse.jdt.junit.JUNIT_CONTAINER/5"/>

	<!-- Percorso corretto della cartella di output -->
